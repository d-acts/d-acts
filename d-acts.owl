                                <owl:onProperty rdf:resource="&obo;IAO_0021011"/>
                                <owl:someValuesFrom rdf:resource="&obo;IAO_0021004"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&obo;IAO_0021012"/>
                                <owl:someValuesFrom rdf:resource="&obo;IAO_0021004"/>
                            </owl:Restriction>
