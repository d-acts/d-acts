    <!-- http://purl.obolibrary.org/obo/IAO_0021002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IAO_0021002">
        <rdfs:subClassOf rdf:resource="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass"/>
