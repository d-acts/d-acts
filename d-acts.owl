    <!-- http://purl.obolibrary.org/obo/IAO_0020022 -->

    <owl:Class rdf:about="&obo;IAO_0020022">
        <rdfs:label xml:lang="en">documented identity</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;IAO_0000030"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&obo;BFO_0000051"/>
                        <owl:someValuesFrom rdf:resource="&obo;IAO_0000030"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty>
