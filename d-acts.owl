        <obo:IAO_0000117 xml:lang="en">Neil Otte</obo:IAO_0000117>
        <rdfs:label xml:lang="en">deontic role</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0021009 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IAO_0021009">
