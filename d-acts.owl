    <!-- http://purl.obolibrary.org/obo/RO_0002218 -->

    <owl:ObjectProperty rdf:about="&obo;RO_0002218">
        <rdfs:label xml:lang="en">has active participant</rdfs:label>
        <obo:IAO_0000412 rdf:datatype="&xsd;string">http://purl.obolibrary.org/obo/ro.owl</obo:IAO_0000412>
        <obo:IAO_0000112>&apos;heart development&apos; has active participant some Shh protein</obo:IAO_0000112>
        <obo:IAO_0000118>has agent</obo:IAO_0000118>
        <obo:IAO_0000117>Chris Mungall</obo:IAO_0000117>
        <obo:IAO_0000116>This may be obsoleted and replaced by the original &apos;has agent&apos; relation</obo:IAO_0000116>
        <obo:IAO_0000115>x has participant y if and only if x realizes some active role that inheres in y</obo:IAO_0000115>
        <obo:IAO_0000114 rdf:resource="&obo;IAO_0000125"/>
        <rdfs:subPropertyOf rdf:resource="&obo;RO_0000057"/>
        <oboInOwl:inSubset rdf:resource="&obo;ro/subsets#ro-eco"/>
    </owl:ObjectProperty>
    


