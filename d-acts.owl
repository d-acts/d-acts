        <obo:IAO_0000117>J. Neil Otte</obo:IAO_0000117>
        <obo:IAO_0000117>Mathias Brochhausen</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">Hohfeld, WN: Fundamental Legal Conceptions as Applied in Judicial Reasoning. Yale University Press, 1919.</obo:IAO_0000119>
        <obo:IAO_0000119>https://plato.stanford.edu/entries/rights/</obo:IAO_0000119>
        <rdfs:label xml:lang="en">duty holder role</rdfs:label>
    </owl:Class>
    


